<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Album</th>
      <th>Author</th>
      <th>Time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.album.name %></td>
        <td><%= song.composer %></td>
        <td><%= song.time %></td>
        <td><%= link_to 'Show', admin_song_path(song) %></td>
        <td><%= link_to 'Edit', edit_admin_song_path(song) %></td>
        <td><%= link_to 'Destroy', admin_song_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</form>
</div>
</div>
</div>
